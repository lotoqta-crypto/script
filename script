    {threshold = 0, case = "Beggar", name = "Beggar (0-4999â­)"},
    {threshold = 5000, case = "Plodder", name = "Plodder (5000-9999â­)"},
    {threshold = 10000, case = "Director", name = "Director (10000-19999â­)"},
